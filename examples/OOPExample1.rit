Rectangle :: BaseType

  width = Integer
  height = Integer

  ::: Integer width, Integer height
    $.width = width
    $.height = height

  area -> Integer: = width * height


Square :: Rectangle

    ::: Integer edge
        width = edge
        height = edge

NamedSquare :: Square
    name = String

    ::: Integer edge, String name
        width = edge
        height = edge
        $.name = name



