Rectangle :: BaseType

  width = int
  height = int

  ::: int width, int height
    $.width = width
    $.height = height

  area -> int: = width * height


Square :: Rectangle

    ::: int edge
        width = edge
        height = edge

NamedSquare :: Square
    name = String

    ::: int edge, String name
        width = edge
        height = edge
        $.name = name



