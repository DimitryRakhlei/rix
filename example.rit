Rectangle :: BaseType
  w = Integer
  h = Integer

  ::: Integer w, Integer h
    $.w = w
    $.h = h

  area -> Integer: = w * h

Square :: Rectangle
  ::: Integer edge
    w = edge
    h = edge

#r = Rectangle 5, 10
#s = Square 16

print ("Rectange area " + (r area))
print ("Square Area " + (s area))
