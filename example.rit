Rectangle :: BaseType

  width = Integer
  height = Integer

  ::: Integer width, Integer height
    $.width = width
    $.height = height

  area -> Integer: = width * height


Square :: Rectangle

  ::: Integer edge
    width = edge
    height = edge


#r = Rectangle 5, 10
#s = Square 16

print ("Rectange area " + (r area))
print ("Square Area " + (s area))
